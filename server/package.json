{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon -L --inspect=0.0.0.0 --exec babel-node src/app.js",
    "lint": "eslint ./",
    "db:migration:create": "npx sequelize-cli migration:create --name",
    "db:migration:up": "npx sequelize-cli db:migrate",
    "db:migration:undo": "npx sequelize-cli db:migrate:undo",
    "db:migration:down": "npx sequelize-cli db:migrate:undo:all",
    "db:seed:create": "npx sequelize-cli seed:generate --name",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:reseed": "npm run db:migration:down && npm run db:migration:up && npm run db:seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "http-status": "^1.5.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "sequelize": "^6.25.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/node": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@extensionengine/eslint-config": "^3.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-sort": "^1.3.0",
    "eslint-plugin-vue": "^9.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1"
  }
}
